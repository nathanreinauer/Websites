@{
    ViewBag.Title = "Members";
}

@RenderPage("~/Views/Shared/_PartialMenu.cshtml")


<div class="container-fluid">
    <div class="row">
        <div class="col-sm-1 hidden-xs"></div>
        <div class="col-sm-10 col-xs-12" style="padding: 0; border: 4px solid #65743A;">

            <div class="members-page">

                <h2 class="members-title">2015 List of Chamber Members</h2>

                <div class="list-section">

                    <ul class="list-unstyled">
                        @Html.Raw(ViewBag.OddList)
                    </ul>

                    <ul class="list-unstyled">
                        @Html.Raw(ViewBag.EvenList)
                    </ul>

                </div>

            </div>

        </div>
    </div>
</div>

